{
  "__inputs": [
    {
      "name": "DS_PROMETHEUS",
      "label": "Prometheus",
      "type": "datasource",
      "pluginId": "prometheus",
      "pluginName": "Prometheus"
    },
    {
      "name": "DS_LOKI",
      "label": "Loki",
      "type": "datasource",
      "pluginId": "loki",
      "pluginName": "Loki"
    }
  ],
  "title": "Mycorrhizal",
  "description": "A Dashboard to vidualize all the Data and information regarding the used Mycorrhizal library in the applications.",
  "uid": "mycorrhizal-main",
  "schemaVersion": 38,
  "version": 1,
  "refresh": "10s",
  "tags": ["mycorrhizal", "cache"],
  "time": { "from": "now-1h", "to": "now" },
  "templating": {
    "list": [
      {
        "name": "service_name",
        "label": "Service Name",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(mycorrizal_cache_gets_total, service_name)",
        "multi": false,
        "includeAll": false,
        "refresh": 2
      },
      {
        "name": "bucket",
        "label": "Bucket",
        "type": "query",
        "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
        "query": "label_values(mycorrizal_cache_gets_total{service_name=\"$service_name\"}, cache_bucket)",
        "multi": true,
        "includeAll": true,
        "allValue": ".*",
        "refresh": 2
      },
      {
        "name": "locality",
        "label": "Locality",
        "type": "custom",
        "query": "local,remote",
        "multi": true,
        "includeAll": true,
        "allValue": ".*"
      }
    ]
  },
  "panels": [
    {
      "id": 1,
      "type": "row",
      "title": "Cache Operations",
      "gridPos": { "x": 0, "y": 0, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 2,
      "type": "timeseries",
      "title": "Gets per Second",
      "description": "Cache get operations per second, split by bucket and locality",
      "gridPos": { "x": 0, "y": 1, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "lineWidth": 2, "fillOpacity": 10 }
        }
      },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum by (cache_bucket, cache_locality) (rate(mycorrizal_cache_gets_total{service_name=\"$service_name\",cache_bucket=~\"$bucket\", cache_locality=~\"$locality\"}[1m]))",
          "legendFormat": "{{cache_bucket}} / {{cache_locality}}"
        }
      ]
    },
    {
      "id": 3,
      "type": "timeseries",
      "title": "Sets per Second",
      "description": "Cache set operations per second, split by bucket and locality",
      "gridPos": { "x": 12, "y": 1, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "lineWidth": 2, "fillOpacity": 10 }
        }
      },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum by (cache_bucket, cache_locality) (rate(mycorrizal_cache_sets_total{service_name=\"$service_name\",cache_bucket=~\"$bucket\", cache_locality=~\"$locality\"}[1m]))",
          "legendFormat": "{{cache_bucket}} / {{cache_locality}}"
        }
      ]
    },
    {
      "id": 4,
      "type": "timeseries",
      "title": "Deletes per Second",
      "gridPos": { "x": 0, "y": 9, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "lineWidth": 2, "fillOpacity": 10 }
        }
      },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum by (cache_bucket, cache_locality) (rate(mycorrizal_cache_deletes_total{service_name=\"$service_name\",cache_bucket=~\"$bucket\", cache_locality=~\"$locality\"}[1m]))",
          "legendFormat": "{{cache_bucket}} / {{cache_locality}}"
        }
      ]
    },
    {
      "id": 5,
      "type": "timeseries",
      "title": "TTL Updates per Second",
      "gridPos": { "x": 8, "y": 9, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "lineWidth": 2, "fillOpacity": 10 }
        }
      },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum by (cache_bucket, cache_locality) (rate(mycorrizal_cache_ttl_updates_total{service_name=\"$service_name\",cache_bucket=~\"$bucket\", cache_locality=~\"$locality\"}[1m]))",
          "legendFormat": "{{cache_bucket}} / {{cache_locality}}"
        }
      ]
    },
    {
      "id": 6,
      "type": "timeseries",
      "title": "Errors per Second",
      "gridPos": { "x": 16, "y": 9, "w": 8, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "color": { "fixedColor": "red", "mode": "fixed" },
          "custom": { "lineWidth": 2, "fillOpacity": 10 }
        }
      },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum by (cache_bucket, cache_locality) (rate(mycorrizal_cache_errors_total{service_name=\"$service_name\",cache_bucket=~\"$bucket\", cache_locality=~\"$locality\"}[1m]))",
          "legendFormat": "{{cache_bucket}} / {{cache_locality}}"
        }
      ]
    },
    {
      "id": 7,
      "type": "row",
      "title": "Hit Rate & Latency",
      "gridPos": { "x": 0, "y": 17, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 8,
      "type": "timeseries",
      "title": "Hit Rate by Bucket",
      "description": "Ratio of hits to total gets. Higher is better.",
      "gridPos": { "x": 0, "y": 18, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "min": 0,
          "max": 1,
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "red", "value": 0 },
              { "color": "yellow", "value": 0.7 },
              { "color": "green", "value": 0.9 }
            ]
          },
          "custom": { "lineWidth": 2, "fillOpacity": 10 }
        }
      },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum by (cache_bucket) (rate(mycorrizal_cache_hits_total{service_name=\"$service_name\", cache_bucket=~\"$bucket\"}[1m])) / sum by (cache_bucket) (rate(mycorrizal_cache_gets_total{service_name=\"$service_name\", cache_bucket=~\"$bucket\"}[1m]))",
          "legendFormat": "{{cache_bucket}}"
        }
      ]
    },
    {
      "id": 9,
      "type": "timeseries",
      "title": "Operation Latency p50 / p95 / p99",
      "description": "End-to-end operation latency split by locality",
      "gridPos": { "x": 12, "y": 18, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ms",
          "custom": { "lineWidth": 2, "fillOpacity": 5 }
        }
      },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "expr": "histogram_quantile(0.50, sum by (le, cache_locality) (rate(mycorrizal_cache_duration_bucket{service_name=\"$service_name\",cache_bucket=~\"$bucket\", cache_locality=~\"$locality\"}[1m])))",
          "legendFormat": "p50 / {{cache_locality}}"
        },
        {
          "expr": "histogram_quantile(0.95, sum by (le, cache_locality) (rate(mycorrizal_cache_duration_bucket{service_name=\"$service_name\",cache_bucket=~\"$bucket\", cache_locality=~\"$locality\"}[1m])))",
          "legendFormat": "p95 / {{cache_locality}}"
        },
        {
          "expr": "histogram_quantile(0.99, sum by (le, cache_locality) (rate(mycorrizal_cache_duration_bucket{service_name=\"$service_name\",cache_bucket=~\"$bucket\", cache_locality=~\"$locality\"}[1m])))",
          "legendFormat": "p99 / {{cache_locality}}"
        }
      ]
    },
    {
      "id": 10,
      "type": "row",
      "title": "Bucket Health",
      "gridPos": { "x": 0, "y": 26, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 11,
      "type": "timeseries",
      "title": "Bucket Size",
      "description": "Current number of items per bucket",
      "gridPos": { "x": 0, "y": 27, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "short",
          "custom": { "lineWidth": 2, "fillOpacity": 10 }
        }
      },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "expr": "mycorrizal_cache_bucket_size{service_name=\"$service_name\", cache_bucket=~\"$bucket\"}",
          "legendFormat": "{{cache_bucket}}"
        }
      ]
    },
    {
      "id": 12,
      "type": "timeseries",
      "title": "Evictions per Second",
      "description": "LRU and TTL evictions per second by bucket",
      "gridPos": { "x": 12, "y": 27, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "lineWidth": 2, "fillOpacity": 10 }
        }
      },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum by (cache_bucket) (rate(mycorrizal_cache_lru_evictions_total{service_name=\"$service_name\", cache_bucket=~\"$bucket\"}[1m]))",
          "legendFormat": "LRU / {{cache_bucket}}"
        },
        {
          "expr": "sum by (cache_bucket) (rate(mycorrizal_cache_ttl_evictions_total{service_name=\"$service_name\", cache_bucket=~\"$bucket\"}[1m]))",
          "legendFormat": "TTL / {{cache_bucket}}"
        }
      ]
    },
    {
      "id": 13,
      "type": "timeseries",
      "title": "Eviction Pressure (evictions / sets)",
      "description": "If this ratio climbs toward 1, the bucket maxLen is too small for the write rate",
      "gridPos": { "x": 0, "y": 35, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "percentunit",
          "thresholds": {
            "mode": "absolute",
            "steps": [
              { "color": "green", "value": 0 },
              { "color": "yellow", "value": 0.5 },
              { "color": "red", "value": 0.8 }
            ]
          },
          "custom": { "lineWidth": 2, "fillOpacity": 10 }
        }
      },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum by (cache_bucket) (rate(mycorrizal_cache_lru_evictions_total{service_name=\"$service_name\", cache_bucket=~\"$bucket\"}[1m])) / sum by (cache_bucket) (rate(mycorrizal_cache_sets_total{service_name=\"$service_name\", cache_bucket=~\"$bucket\"}[1m]))",
          "legendFormat": "{{cache_bucket}}"
        }
      ]
    },
    {
      "id": 14,
      "type": "timeseries",
      "title": "Hits vs Misses",
      "gridPos": { "x": 12, "y": 35, "w": 12, "h": 8 },
      "datasource": { "type": "prometheus", "uid": "${DS_PROMETHEUS}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "lineWidth": 2, "fillOpacity": 10 }
        }
      },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum by (cache_bucket) (rate(mycorrizal_cache_hits_total{service_name=\"$service_name\", cache_bucket=~\"$bucket\"}[1m]))",
          "legendFormat": "hits / {{cache_bucket}}"
        },
        {
          "expr": "sum by (cache_bucket) (rate(mycorrizal_cache_misses_total{service_name=\"$service_name\", cache_bucket=~\"$bucket\"}[1m]))",
          "legendFormat": "misses / {{cache_bucket}}"
        }
      ]
    },
    {
      "id": 20,
      "type": "row",
      "title": "Logs",
      "gridPos": { "x": 0, "y": 43, "w": 24, "h": 1 },
      "collapsed": false
    },
    {
      "id": 21,
      "type": "logs",
      "title": "All Logs",
      "description": "All structured logs from the mycorrizal library",
      "gridPos": { "x": 0, "y": 44, "w": 24, "h": 12 },
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false,
        "showCommonLabels": false,
        "showLabels": true,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": false
      },
      "targets": [
        {
          "expr": "{service_name=\"$service_name\"} | json",
          "legendFormat": ""
        }
      ]
    },
    {
      "id": 22,
      "type": "logs",
      "title": "Errors Only",
      "description": "Logs at ERROR level only",
      "gridPos": { "x": 0, "y": 56, "w": 24, "h": 12 },
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "options": {
        "dedupStrategy": "none",
        "enableLogDetails": true,
        "prettifyLogMessage": false,
        "showCommonLabels": false,
        "showLabels": true,
        "showTime": true,
        "sortOrder": "Descending",
        "wrapLogMessage": false
      },
      "targets": [
        {
          "expr": "{service_name=\"$service_name\"} | json | level=\"ERROR\"",
          "legendFormat": ""
        }
      ]
    },
    {
      "id": 23,
      "type": "timeseries",
      "title": "Log Rate by Level",
      "description": "Volume of log lines per second by severity level",
      "gridPos": { "x": 0, "y": 68, "w": 24, "h": 8 },
      "datasource": { "type": "loki", "uid": "${DS_LOKI}" },
      "fieldConfig": {
        "defaults": {
          "unit": "ops",
          "custom": { "lineWidth": 2, "fillOpacity": 10 }
        },
        "overrides": [
          {
            "matcher": { "id": "byName", "options": "ERROR" },
            "properties": [{ "id": "color", "value": { "fixedColor": "red", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "WARN" },
            "properties": [{ "id": "color", "value": { "fixedColor": "orange", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "INFO" },
            "properties": [{ "id": "color", "value": { "fixedColor": "blue", "mode": "fixed" } }]
          },
          {
            "matcher": { "id": "byName", "options": "DEBUG" },
            "properties": [{ "id": "color", "value": { "fixedColor": "gray", "mode": "fixed" } }]
          }
        ]
      },
      "options": { "legend": { "displayMode": "table", "placement": "bottom" } },
      "targets": [
        {
          "expr": "sum by (level) (rate({service_name=\"$service_name\"} | json | __error__=\"\" [1m]))",
          "legendFormat": "{{level}}"
        }
      ]
    }
  ]
}
